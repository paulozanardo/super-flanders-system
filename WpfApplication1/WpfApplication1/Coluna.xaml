<Window x:Class="ProgramaModificaArquivoZip.Coluna"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Selecionar Coluna XLS" Height="73.648" Width="300" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid Margin="0,0,0,-29">
        <Button x:Name="button" Content="OK" HorizontalAlignment="Left" Margin="197,10,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Grid Margin="10,15,121,15">
            <RadioButton x:Name="radioButtonA" Content="A" HorizontalAlignment="Left" VerticalAlignment="Top" />
            <RadioButton x:Name="radioButtonB" Content="B" HorizontalAlignment="Left" Margin="40,0,0,0" IsChecked="True" VerticalAlignment="Top"/>
            <RadioButton x:Name="radioButtonC" Content="C" HorizontalAlignment="Left" Margin="80,0,0,0" VerticalAlignment="Top"/>
            <RadioButton x:Name="radioButtonD" Content="D" HorizontalAlignment="Left" Margin="120,0,0,0" VerticalAlignment="Top"/>

        </Grid>

    </Grid>
</Window>
